<p id="notice"><%= notice %></p>

<h1>Posts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <div>
        <p><%= post.title %></p>
        <p><%= post.body %></p>
        <% if current_user %>
        <p>posted by <%= post.user.name %></p>
        <% end %>
      </div>
    <% end %>

  </tbody>
</table>

<br>

<%#= link_to 'New Post', new_post_path %>
